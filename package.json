  "private": true,                    // 私有项目，禁止意外发布到npm
  "version": "0.0.0",                 // 初始版本号
  "type": "module",                   // 使用ES模块系统
  // 脚本命令配置
  "scripts": {
    "dev": "vite",                    // 开发服务器启动命令
    "build": "vite build",            // 生产环境构建命令
    "preview": "vite preview"         // 预览构建结果命令
